{
  "compilerOptions": {
    "target": "ESNext",
    "module": "Node16",
    "moduleResolution": "node16",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "../../dist/quantum",
    "rootDir": "./src",
    "resolveJsonModule": true,
    "noEmitOnError": true,
    "isolatedModules": true,
    "allowUnreachableCode": true,
    "skipLibCheck": true,
    "noCheck": false,
    "types": [
      
    ],
    "allowSyntheticDefaultImports": true,
    "baseUrl": "./"
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules/**/*.mts",
    "dist/**/*.mjs",
    "**/*.test.ts",
    "**/*.spec.ts"
  ]
}
